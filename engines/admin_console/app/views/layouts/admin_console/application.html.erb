<!DOCTYPE html>
<html>
<head>
  <title>AdminConsole</title>
  <%= stylesheet_link_tag    'admin_console/application', media: 'all' %>
  <%= javascript_include_tag 'admin_console/application', params[:controller] %>
  <%= csrf_meta_tags %>
</head>
<body ng-app="app">

<div id="wrapper">
  <div id="sidebar-wrapper" ng-controller="NavCtrl">
    <ul class="sidebar-nav">
      <li><a href="#">Admin Console Home</a></li>
      <li class="dropdown">
        <a data-toggle="dropdown" class="dropdown-toggle">MAUVSA Advance Conference</a>
        <ul class="dropdown-menu">
          <li><a href="#">Conferences</a></li>
          <li><a href="#">Staff</a></li>
          <li><a href="#">Activities</a></li>
        </ul>
      </li>
      <li class="dropdown">
        <a data-toggle="dropdown" class="dropdown-toggle">Manage Organizations</a>
        <ul class="dropdown-menu">
          <li><a href="#">Organizations</a></li>{{text}}
          <li><a href="#">Board Members</a></li>
        </ul>
      </li>
      <li><a href="#">Payments</a></li>
    </ul>
  </div>

  <div id="page-content-wrapper">
    <div class="container-fluid">
      <nav class="navbar navbar-default navbar-fixed-top"></nav>
      <%= yield %>
    </div>
  </div>

</div>

</body>
</html>